<Application x:Class="TusurUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TusurUI"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles.xaml"/>
                <ResourceDictionary x:Name="MainWindowLanguageResources" Source="Resources/MainWindowUIElements.ru.xaml"/>
                <ResourceDictionary x:Name="ScenarioWindowLanguageResources" Source="Resources/ScenarioWindowUIElements.ru.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
